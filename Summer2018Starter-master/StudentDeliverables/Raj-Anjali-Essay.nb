    RowBox[{
     SuperscriptBox["M", "e"], 
     RowBox[{"(", 
      RowBox[{"mod", " ", "n"}], ")"}]}]}], "Text"], "\n", 
  StyleBox[
   RowBox[{"M", "  ", "\[Congruent]", "  ", 
    RowBox[{
     SuperscriptBox["E", "d"], 
     RowBox[{"(", 
      RowBox[{"mod", " ", "n"}], ")"}]}]}], "Text"]}], "Code",
  CellChangeTimes->{{3.739224479300432*^9, 3.739224493554137*^9}, {
   3.7392245488044777`*^9, 3.7392245882572985`*^9}, {3.739224655970358*^9, 
   3.739224656861301*^9}, {3.7392246877306714`*^9, 3.739224696944826*^9}, {
